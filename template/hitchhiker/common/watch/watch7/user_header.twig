<div id="watch7-user-header" class=" spf-link ">
    <a href="{{ owner.navigationEndpoint.browseEndpoint.canonicalBaseUrl }}" class="yt-user-photo yt-uix-sessionlink      spf-link " data-sessionlink="itct=CDoQ4TkiEwjwm97Es-raAhXLon4KHc-dDBgo-B0" >
    <span class="video-thumb  yt-thumb yt-thumb-48">
        <span class="yt-thumb-square">
            <span class="yt-thumb-clip">
                <img data-ytimg="1" width="48" height="48" data-thumb="{{ owner.thumbnail.thumbnails.0.url }}" alt="{{ getText(owner.title) }}" src="{{ PIXEL }}" onload=";window.__ytRIL &amp;&amp; __ytRIL(this)" >
                <span class="vertical-align"></span>
            </span>
        </span>
    </span>
    </a>
    <div class="yt-user-info">
    <a href="{{ owner.navigationEndpoint.browseEndpoint.canonicalBaseUrl }}" class="yt-uix-sessionlink       spf-link " data-sessionlink="itct=CDoQ4TkiEwjwm97Es-raAhXLon4KHc-dDBgo-B0" >{{ getText(owner.title) }}</a>
    {% if owner.badges %}
    <span data-tooltip-text="Verified" class="yt-channel-title-icon-verified yt-uix-tooltip yt-sprite" aria-label="Verified"></span>
    {% endif %}
    </div>
    <span id="watch7-subscription-container">
    {% import 'common/uix/subscription_button.twig' as subscription_button %}
    {{ subscription_button.render(owner.subscriptionButtonRenderer) }}
    </span>
</div>