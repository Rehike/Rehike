      <div id="appbar-main-guide-notification-container"></div>
      {% endif %}
      <span id="yt-masthead-logo-fragment"><a href="/" class="masthead-logo-renderer yt-uix-sessionlink      spf-link " data-sessionlink="itct=CAUQsV4iEwji18z73-raAhUQO38KHYD9Cikojh4"  id="logo-container" title="YouTube Home">  <span title="YouTube Home" class="logo masthead-logo-renderer-logo yt-sprite"></span>
      </a></span>
   </div>
   <div id="yt-masthead-{{ yt.signin.isSignedIn ? 'user' : 'signin' }}">
