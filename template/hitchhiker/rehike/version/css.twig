body.rehike-version-page
{
    min-width: 850px;
}

.rehike-version-page .yt-base-gutter
{
    min-width: 790px;
}

#rehike-version .header
{
    border-bottom: 1px solid #e2e2e2;
    padding: 12px;
    height: 30px;
}

#rehike-version .header-left
{
    float: left;
}

#rehike-version .header-left .header-text
{
    margin-top: 3px;
}

#rehike-version .header-right
{
    float: right;
}

#rehike-version-content
{
    padding: 12px;
}

#rehike-brand-header
{
    margin-bottom: 16px;
}

#rehike-brand-header .buttons
{
    margin-top: 16px;
    margin-left: 12px;
}

#rehike-brand-header .buttons > *
{
    margin-left: 4px;
}

#rehike-brand-header .buttons > *:first-child
{
    margin-left: 0;
}

.rehike-brand-text, .rehike-brand-nightly
{
    color: #333;
    font-weight: 500;
    font-size: 32px;
}

.rehike-brand-nightly
{
    color: #62287b;
    font-weight: 700;
    transition: ease-in color 100ms;
}

.rehike-brand-nightly:hover
{
    color: #7c339b;
}

.rehike-version-logo-content
{
    display: inline-block;
    vertical-align: middle;
    margin-left: 20px;
    font-size: 32px;
}

.rehike-version-text
{
    color: #565656;
    font-size: 16px;

    /* Position under the Rehike logo */
    position: absolute;
    bottom: 20px;
    left: 167px;
}

.rehike-version-notice
{
    border-top: 1px solid #e2e2e2;
    text-align: center;
    margin-top: 12px;
    padding-top: 12px;
}

.rehike-version-notice > *
{
    margin: 0 auto;
}

.rehike-version-notice:first-child
{
    margin-top: 0;
    padding-top: 0;
    border-top: none;
}

.rehike-version-notice .notice-header
{
    width: 700px;
    font-size: 26px;
    color: #000;
    margin-bottom: 6px;
}

.rehike-version-notice .notice-description
{
    display: block;
    width: 600px;
    font-size: 16px;
    color: #666;
}

#rehike-info-nightly .commit-head
{
    margin-top: 6px;
    line-height: 20px;
    /* temporary */
    margin-left: 60px;
}

#rehike-info-nightly .commit-name
{
    color: #000;
}

#rehike-info-nightly .commit-branch
{
    color: #111;
    font-weight: 500;
}

#rehike-info-nightly .commit-branch::after
{
    content: "â€¢";
    margin: 0px 4px;
    font-weight: 500;
    color: #000;
}

#rehike-info-nightly .update-container
{
    margin-top: 12px;
}

#rehike-info-nightly .update-container .title
{
    font-weight: 500;
    color: #333;
}

#rehike-info-nightly .update-container.update-available .commit
{
    background: #fdf3d0;
    border-left: 6px solid #f5d562;
    color: #333;
    padding: 6px;
    margin-top: 8px;
}

.rehike-version .commit-hash
{
    font-family: monospace;
    background: #eee;
    border: 1px solid #ddd;
    padding: 1px 2px;
    display: inline;
}

.rehike-version .commit-name
{
    font-size: 18px;
}

.rehike-version .commit-time
{
    display: block;
    color: #333;
}

.rehike-version .commit-buttons *
{
    margin-left: 4px;
}

.rehike-version .commit-buttons *:first-child
{
    margin-left: 0;
}

.rehike-version-mini-logo
{
    background: url(/rehike/static/version/logo_small_grey.png);
    width: 89px;
    height: 30px;
}

.yt-uix-button-icon-rh-github-icon
{
    background: no-repeat url(/rehike/static/version/version_logos.png) -60px -415px;
    width: 16px;
    height: 16px;
}

.yt-uix-button-dark .yt-uix-button-icon-rh-github-icon
{
    background: no-repeat url(/rehike/static/version/version_logos.png) -42px -415px;
    width: 16px;
    height: 16px;
}

.yt-uix-button-dark .yt-uix-button-icon.yt-uix-button-icon-rh-github-icon
{
    opacity: 90%;
}

.yt-uix-button-dark .yt-uix-button-icon.yt-uix-button-icon-rh-github-icon:hover
{
    opacity: 100%;
}

.rehike-version-logo
{
    background: no-repeat url(/rehike/static/version/version_logos.png);
    width: 700px;
    height: 179px;
    vertical-align: middle;
    position: relative;
}

.rehike-version-logo .nightly-logo
{
    background: no-repeat url(/rehike/static/version/version_logos.png) 0 -179px;
    width: 249px;
    height: 113px;
    vertical-align: middle;
    position: relative;

    margin-left: 436px;
    margin-top: 28px;
}

.branch-icon
{
    background: no-repeat url(/rehike/static/version/version_logos.png) -95px -358px;
    width: 42px;
    height: 38px;
    position: absolute;
    margin-top: 8px;
    margin-left: 5px;
}

.update-container .branch-icon
{
    background: no-repeat url(/rehike/static/version/version_logos.png) 0 -412px;
    width: 42px;
    height: 38px;
}

#rehike-runtime-info h1 {
    margin-top: 6px;
}

.extra-info
{
    border-collapse: separate;
    border-spacing: 8px 4px;
    margin-left: -8px;
}

.rehike-info
{
    margin: 0 12px;
    padding: 12px 0;
    border-top: 1px solid #e2e2e2;
}

.rehike-info h1
{
    color: #333;
    font-size: 18px;
    font-weight: 500;
    margin-bottom: 12px;
}

.info-nightly-indicator
{
    margin-left: 6px;
    font-size: 11px;
    font-weight: 500;
    text-transform: uppercase;
    color: #62287b;
    border: 1px solid #62287b;
    padding: 2px 4px;
    vertical-align: middle;
}

.info-nightly-indicator-title
{
    vertical-align: middle;
}

/* ======================================================== */
/* ======================= CREDITS ======================== */
/* ======================================================== */

.rehike-credits-content
{
    overflow-y: auto;

    /* In case the content overflows, we don't want it looking weird
       if the browser scrollbar is visible. */
    padding-right: 16px;
}

.rehike-credits-content .section
{
    margin-top: 12px;
}

.rehike-credits-content .section:first-child
{
    margin-top: 0;
}

.rehike-credits-content .title
{
    font-size: 15px;
    font-weight: 500;
    margin-bottom: 6px;
}

.rehike-credits-content .developers-section .left,
.rehike-credits-content .developers-section .right
{
    display: inline-block;
    vertical-align: middle;
}

.rehike-credits-content .developers-section .photo
{
    margin-right: 10px;
}

.rehike-credits-content .general-section ul
{
    list-style: inside;
}

.rehike-credits-content .info-buttons
{
    margin: 5px 0;
}

