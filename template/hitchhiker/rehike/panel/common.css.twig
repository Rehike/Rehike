.rehike-version-mini-logo {
    background: url(/rehike/static/version/logo_small_grey.png);
    width: 89px;
    height: 30px;
}

.rehike-creator-header-right {
    position: absolute;
    top: 20px;
    right: 35px;
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-version {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) 0 0;
    background-size: auto;
    width: 24px;
    height: 24px
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-version.selected,
#creator-sidebar a:hover .studio-icon.creator-sidebar-rehike-version.selected {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) -24px 0;
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-appearance {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) 0 -24px;
    background-size: auto;
    width: 24px;
    height: 24px
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-appearance.selected,
#creator-sidebar a:hover .studio-icon.creator-sidebar-rehike-appearance.selected {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) -24px -24px;
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-experiments {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) 0 -48px;
    background-size: auto;
    width: 24px;
    height: 24px
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-experiments.selected,
#creator-sidebar a:hover .studio-icon.creator-sidebar-rehike-experiments.selected {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) -24px -48px ;
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-advanced {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) 0 -72px;
    background-size: auto;
    width: 24px;
    height: 24px
}

#creator-sidebar .studio-icon.creator-sidebar-rehike-advanced.selected,
#creator-sidebar a:hover .studio-icon.creator-sidebar-rehike-advanced.selected {
    background: no-repeat url(/rehike/static/rehike/www-sidebar.png) -24px -72px;
